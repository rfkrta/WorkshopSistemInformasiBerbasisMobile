<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.firstapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Tugas Mobile Aplication"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirstApp">
        <activity
            android:name=".recyclerview"
            android:exported="true"
            android:label="Recycler View"/>
        <activity
            android:name=".autocomplete"
            android:exported="true"
            android:label="Auto Complete View"/>
        <activity
            android:name=".spinneract"
            android:exported="true"
            android:label="Spinner"/>
        <activity
            android:name=".listviewact"
            android:exported="true"
            android:label="List View"/>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>